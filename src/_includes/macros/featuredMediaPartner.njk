{% macro fmpComp(thisPartner) %}

      <div class="carousel">
            <div class="slides">
                  {% for work in thisPartner.works %}
                        <div class="carouselSlide sevenFiveGrid">
                              <div class="slideLeft" style="background-image: url({{ work.image.url }}); background-size: cover; background-position: center;"></div>
                              <div class="slideRight">
                                    <div>
                                          <div class="bm3">{{work.typeOfMedia}}</div>
                                          <div class="vertGap1"></div>
                                          <div class="h1_0">{{work.title}}</div>
                                          <div class="vertGap1"></div>
                                          <div class="bs1">{{work.description}}</div>
                                          <div class="vertGap1"></div>
                                          <button class="ctaButton ">
                                                <a target="_blank" href="{{work.ctaButton.link}}" >
                                                      {{work.ctaButton.name}}
                                                </a>
                                          </button>
                                    </div>
                                    <div class="controls">
                                          <button class="prevButton"></button>
                                          <button class="nextButton"></button>
                                    </div>
                              </div>
                        </div>
                  {% endfor %}
                  {# <img src="images/image1.jpg" alt="Image 1"> #}
                  {# <img src="images/image2.jpg" alt="Image 2"> #}
                  {# <img src="images/image3.jpg" alt="Image 3"> #}
            </div>
            <button class="prev">Prev</button>
            <button class="next">Next</button>
      </div>
{% endmacro %}